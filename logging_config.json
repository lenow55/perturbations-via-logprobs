{
  "version": 1,
  "formatters": {
    "default": {
      "format": "%(asctime)s ==%(levelname)s== %(name)s:%(funcName)s:%(lineno)d ==--== %(message)s"
    }
  },
  "handlers": {
    "default": {
      "class": "logging.StreamHandler",
      "formatter": "default",
      "level": "DEBUG",
      "stream": "ext://sys.stderr"
    }
  },
  "loggers": {
    "__main__": {
      "level": "DEBUG",
      "handlers": [
        "default"
      ]
    },
    "src": {
      "level": "DEBUG",
      "handlers": [
        "default"
      ]
    }
  }
}
